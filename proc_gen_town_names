# it is a simple procedural generator that makes a town and a village name

import random

directions = ["North", "South", "East", "West", "New"]
prefix = ["Edgar", "Hart", "Vine","Tis", "Eng", "Lon", "War"]
suffix = ["yard", "town", "ville", "ton", "land", "burg", "falls", "shire", "don"]

for i in range(10):
    village_name = random.choice(directions) + " " + random.choice(prefix) + random.choice(suffix)
    print(village_name)
